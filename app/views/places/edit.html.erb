<%= form_for @place, :html => { :multipart => true } do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br>
	<%= f.label :address %>
	<%= f.text_field :address %>
	<br>
	<%= f.label :offers %>
	<%= f.text_field :offers %>
	<br>
	<%= f.label :image %>
	<%= f.file_field :image %>
	<br>
	<%= f.submit 'Update' %>
<% end %>