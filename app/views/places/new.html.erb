<div class="container">
    <div class="page-header">
        <div class="col-md-12">
            <h1>
            	Registro como establecimiento
            </h1>
        </div>
    </div>
</div>
<div class="container">
	<form class="col-md-4"></form>
	<div class="col-md-4">
	<%= form_for @place, :html => { :multipart => true } do |f| %>
		<% if @place.errors.any? %>
		<ul>
			<% @place.errors.full_messages.each do |error_msg| %>
			<li><%= error_msg %></li>
			<% end %>
		</ul>
		<% end %>
	    <div class="form-group">
	    	<%= f.text_field :name, class: "form-control input-lg", :placeholder => 'Nombre del establecimiento' %>
	    </div>
	    <div class="form-group">
	    	<%= f.password_field :password, class: "form-control input-lg", :placeholder => 'Contraseña' %>
	    </div>
	    <div class="form-group">
	    	<%= f.text_field :city, class: "form-control input-lg", :placeholder => 'Ciudad' %>
	    </div>
	    <div class="form-group">
	    	<%= f.text_field :address, class: "form-control input-lg", :placeholder => 'Dirección' %>
	    </div>
	    <div class="form-group">
	    	<%= f.text_field :timetable, class: "form-control input-lg", :placeholder => 'Horario' %>
	    </div>
	    <div class="form-group">
	    	<%= f.file_field :image_place %>
	    </div>
	    <div class="form-group">
	    	<%= f.submit '¡Crear!', class: "btn btn-primary btn-lg btn-block" %>
	    </div>
	</div>
	<% end %>
	<form class="col-md-4"></form>
</div>